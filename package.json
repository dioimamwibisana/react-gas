{
  "name": "react-gas",
  "version": "1.0.0",
  "description": "Simple react boilerplate/starter-kit for better development using React, Express, React Router, and Webpack",
  "scripts": {
    "clean": "rm -rf ./build",
    "server:build": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.server.js",
    "client:build": "npm run clean && cross-env NODE_ENV=\"production\" webpack",
    "build": "npm run client:build && npm run server:build",
    "production": "npm run build && npm start",
    "dev": "webpack-dev-server",
    "start": "node ./build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dioimamwibisana/react-gas.git"
  },
  "keywords": [
    "react",
    "react-router",
    "express",
    "webpack"
  ],
  "author": "Dio Imam Wibisana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dioimamwibisana/react-gas/issues"
  },
  "homepage": "https://github.com/dioimamwibisana/react-gas#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.6.0",
    "postcss-loader": "^2.0.8",
    "react-svg-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
